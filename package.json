{
  "name": "@mixte/monorepo",
  "version": "1.6.0",
  "private": true,
  "packageManager": "pnpm@8.8.0",
  "description": "",
  "author": "Wei Zhang <https://github.com/Zhang-Wei-666>",
  "license": "MIT",
  "homepage": "https://github.com/MoomFE/mixte#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoomFE/mixte.git"
  },
  "bugs": {
    "url": "https://github.com/MoomFE/mixte/issues"
  },
  "keywords": [],
  "scripts": {
    "test": "pnpm install && vitest --coverage",
    "test:ui": "pnpm test -- --ui",
    "test-build": "pnpm build && vitest --mode test-build",
    "test-build:ui": "pnpm test-build --ui",
    "test-release": "pnpm install && vitest --run && vue-tsc --noEmit && pnpm test-build --run",
    "dev": "pnpm docs:dev",
    "docs:dev": "pnpm udd && vitepress dev packages --host",
    "docs:build": "pnpm udd && pnpm build && vitepress build packages",
    "docs:preview": "pnpm docs:build && vitepress preview packages --host",
    "up": "npx taze latest -r -w -i -x change-case",
    "udd": "pnpm install && tsx scripts/updateDocsDetails.ts",
    "build": "pnpm install && tsx scripts/build.ts",
    "release": "pnpm test-release && npx bumpp -r --no-tag --no-push"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.0.2",
    "@iconify/json": "^2.2.124",
    "@moomfe/eslint-config": "^2.7.2",
    "@moomfe/small-utils": "^5.3.2",
    "@rollup/pluginutils": "^5.0.4",
    "@types/fs-extra": "^11.0.2",
    "@types/js-yaml": "^4.0.6",
    "@types/lodash-es": "^4.17.9",
    "@vercel/analytics": "^1.0.2",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.1",
    "change-case": "4.1.2",
    "element-plus": "^2.3.14",
    "eslint": "^8.50.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "gsap": "^3.12.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "lodash-es": "^4.17.21",
    "magic-string": "^0.30.4",
    "pnpm": "^8.8.0",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^6.0.2",
    "sass": "^1.68.0",
    "shiki": "^0.14.4",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^3.13.0",
    "type-fest": "^4.3.3",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "unocss-preset-extra": "^0.5.2",
    "unocss-preset-scrollbar": "^0.3.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vitepress": "1.0.0-rc.20",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.6",
    "vue-tsc": "^1.8.15"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}
