    "@vitest/ui": "^0.34.4",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "element-plus": "^2.3.12",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.49.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "gsap": "^3.12.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "lodash-es": "^4.17.21",
    "magic-string": "^0.30.3",
    "mixte": "workspace:*",
    "pnpm": "^8.7.5",
    "rollup": "^3.29.1",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "sass": "^1.67.0",
    "shiki": "^0.14.4",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^3.12.10",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7",
    "unocss-preset-extra": "^0.5.2",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vitepress": "1.0.0-rc.13",
    "vitest": "^0.34.4",
    "vue-demi": "^0.14.6",
    "vue-tsc": "^1.8.11"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
