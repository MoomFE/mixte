{
  "name": "@mixte/monorepo",
  "type": "module",
  "version": "1.11.0",
  "private": true,
  "packageManager": "pnpm@8.15.7",
  "description": "",
  "author": "Wei Zhang <https://github.com/Zhang-Wei-666>",
  "license": "MIT",
  "homepage": "https://github.com/MoomFE/mixte#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoomFE/mixte.git"
  },
  "bugs": {
    "url": "https://github.com/MoomFE/mixte/issues"
  },
  "keywords": [],
  "scripts": {
    "test": "pnpm install && vitest --coverage",
    "test:ui": "pnpm test -- --ui",
    "test-build": "pnpm build && vitest --mode test-build",
    "test-build:ui": "pnpm test-build --ui",
    "test-release": "pnpm install && vitest --run && vue-tsc --project tsconfig.tsc.json --noEmit && pnpm test-build --run && pnpm build-dts",
    "test-coverage": "pnpm install && vitest --run",
    "dev": "pnpm docs:dev",
    "docs:dev": "pnpm udd && vitepress dev packages --host",
    "docs:build": "pnpm udd && pnpm build && vitepress build packages",
    "docs:preview": "pnpm docs:build && vitepress preview packages --host",
    "up": "npx taze latest -r -w -i -x",
    "lint": "pnpm install && eslint .",
    "lint:fix": "pnpm install && eslint . --fix",
    "udd": "pnpm install && tsx scripts/updateDocsDetails.ts",
    "build": "pnpm install && tsx scripts/build.ts",
    "build-dts": "pnpm install && tsx scripts/build-dts.ts",
    "release": "pnpm test-release && npx bumpp -r --no-tag --no-push"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.0.6",
    "@iconify/json": "^2.2.203",
    "@mixte/components": "workspace:*",
    "@mixte/snippets": "workspace:*",
    "@mixte/use": "workspace:*",
    "@moomfe/eslint-config": "^3.0.0",
    "@moomfe/small-utils": "^5.3.2",
    "@rollup/pluginutils": "^5.1.0",
    "@shikijs/vitepress-twoslash": "^1.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/postcss-js": "^4.0.4",
    "@vercel/analytics": "^1.2.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "@vue/test-utils": "^2.4.5",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "change-case": "^5.4.4",
    "element-plus": "^2.7.1",
    "eslint": "^9.1.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gsap": "^3.12.5",
    "js-base64": "^3.7.7",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "lodash-es": "^4.17.21",
    "magic-string": "^0.30.10",
    "mixte": "workspace:*",
    "naive-ui": "^2.38.1",
    "pnpm": "^8.15.7",
    "postcss": "^8.4.38",
    "postcss-js": "^4.0.1",
    "rollup": "^4.16.1",
    "rollup-plugin-dts": "^6.1.0",
    "sass": "^1.75.0",
    "scule": "^1.3.0",
    "shiki": "^1.3.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.7.2",
    "type-fest": "^4.17.0",
    "typescript": "^5.4.5",
    "unocss": "^0.59.4",
    "unocss-preset-extra": "^0.5.3",
    "unocss-preset-scrollbar": "^0.3.1",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.10",
    "vitepress": "^1.1.3",
    "vitest": "^1.5.0",
    "vue": "^3.4.21",
    "vue-demi": "^0.14.7",
    "vue-tsc": "^2.0.14"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}
