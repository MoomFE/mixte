  "bugs": {
    "url": "https://github.com/MoomFE/mixte/issues"
  },
  "keywords": [],
  "scripts": {
    "build": "tsx scripts/build.ts",
    "test": "vitest --coverage",
    "test:ui": "vitest --coverage --ui",
    "test-build": "npm run build && vitest --mode test-build",
    "test-build:ui": "vitest --mode test-build --ui",
    "test-release": "vitest --run && npm run build && vitest --mode test-build --run",
    "up": "npx taze latest -r -w -i",
    "release": "npm run test-release && npx bumpp -r --no-tag --no-push",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@moomfe/eslint-config": "^2.6.0",
    "@moomfe/small-utils": "^5.3.2",
    "@rollup/plugin-alias": "^5.0.0",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "@vueuse/core": "^10.2.1",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.45.0",
    "pnpm": "^8.6.8",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "simple-git-hooks": "^2.8.1",
    "tsx": "^3.12.7",
    "type-fest": "^4.0.0",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.4",
    "vitepress": "1.0.0-beta.6",
    "vitest": "^0.33.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}