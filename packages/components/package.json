{
  "name": "@mixte/components",
  "type": "module",
  "version": "3.5.0-beta.3",
  "author": "Wei Zhang <https://github.com/Zhang-Wei-666>",
  "license": "MIT",
  "homepage": "https://github.com/MoomFE/mixte#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoomFE/mixte.git"
  },
  "bugs": {
    "url": "https://github.com/MoomFE/mixte/issues"
  },
  "sideEffects": false,
  "exports": {
    "./auto-grid": {
      "types": "./dist/auto-grid.d.ts",
      "import": "./dist/auto-grid.mjs",
      "require": "./dist/auto-grid.cjs"
    },
    "./infinite-scroll": {
      "types": "./dist/infinite-scroll.d.ts",
      "import": "./dist/infinite-scroll.mjs",
      "require": "./dist/infinite-scroll.cjs"
    },
    "./grid-table": {
      "types": "./dist/grid-table/index.d.ts",
      "import": "./dist/grid-table/index.mjs",
      "require": "./dist/grid-table/index.cjs"
    },
    "./grid-table/*": {
      "types": "./dist/grid-table/*.d.ts",
      "import": "./dist/grid-table/*.mjs",
      "require": "./dist/grid-table/*.cjs"
    },
    "./*": "./*"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "tsx build.ts"
  },
  "peerDependencies": {
    "@mixte/use": "workspace:*",
    "@vueuse/core": ">=8.0.0",
    "mixte": "workspace:*",
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "vue-component-type-helpers": "catalog:"
  }
}
