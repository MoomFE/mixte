  "dependencies": {
    "@tiptap/extension-bubble-menu": "catalog:",
    "@tiptap/extension-underline": "catalog:",
    "@tiptap/pm": "catalog:",
    "@tiptap/starter-kit": "catalog:",
    "@tiptap/vue-3": "catalog:",
    "@types/three": "catalog:",
    "gsap": "catalog:",
    "three": "catalog:"
  }
