    "url": "git+https://github.com/MoomFE/mixte.git"
  },
  "bugs": {
    "url": "https://github.com/MoomFE/mixte/issues"
  },
  "exports": {
    "./getFastestCDN": {
      "types": "./dist/getFastestCDN.d.ts",
      "import": "./dist/getFastestCDN.mjs",
      "require": "./dist/getFastestCDN.cjs"
    },
    "./useNaiveForm": {
      "types": "./dist/useNaiveForm.d.ts",
      "import": "./dist/useNaiveForm.mjs",
      "require": "./dist/useNaiveForm.cjs"
    },
    "./toggleThemeViewTransition": {
      "types": "./dist/toggleThemeViewTransition.d.ts",
      "import": "./dist/toggleThemeViewTransition.mjs",
      "require": "./dist/toggleThemeViewTransition.cjs"
    },
    "./tiptap-editor": {
      "types": "./dist/tiptap-editor.d.ts",
      "import": "./dist/tiptap-editor.mjs",
      "require": "./dist/tiptap-editor.cjs"
    },
    "./tiptap-editor/*": {
      "types": "./dist/tiptap-editor/*.d.ts",
      "import": "./dist/tiptap-editor/*.mjs",
      "require": "./dist/tiptap-editor/*.cjs"
    },
    "./lottery": {
      "types": "./dist/lottery.d.ts",
      "import": "./dist/lottery.mjs",
      "require": "./dist/lottery.cjs"
    },
    "./lottery/*": {
      "types": "./dist/lottery/*.d.ts",
      "import": "./dist/lottery/*.mjs",
      "require": "./dist/lottery/*.cjs"
    },
    "./*": "./*"
