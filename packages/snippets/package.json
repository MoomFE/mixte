  "files": [
    "*.ts",
    "dist"
  ],
  "scripts": {
    "build": "tsx build.ts"
  },
  "peerDependencies": {
    "@mixte/use": ">=3.0.0",
    "@vueuse/core": ">=8.0.0",
    "mixte": ">=3.0.0",
    "vue": ">=3.2.0"
