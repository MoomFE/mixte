{
  "name": "@mixte/use",
  "type": "module",
  "version": "3.5.0-beta.11",
  "author": "Wei Zhang <https://github.com/Zhang-Wei-666>",
  "license": "MIT",
  "homepage": "https://github.com/MoomFE/mixte#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoomFE/mixte.git"
  },
  "bugs": {
    "url": "https://github.com/MoomFE/mixte/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.mjs",
      "require": "./dist/register.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "tsx build.ts"
  },
  "peerDependencies": {
    "@vueuse/core": ">=8.0.0",
    "mixte": "workspace:*",
    "vue": ">=3.2.0"
  }
}
